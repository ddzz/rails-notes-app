<ul class="users_index">
  <p>Logged in as <%= @user.email %>.</p>
  <p>
		<%= link_to "+ New Note", new_note_path, class: "btn btn-primary" %>
    <%= link_to "Go Back", user_path(@user.id), class: "btn btn-primary" %>
		<%= link_to "Log out", logout_path, method: "delete", class: "btn btn-danger" %>
	</p>
  <%= render @notes %>
</ul>
